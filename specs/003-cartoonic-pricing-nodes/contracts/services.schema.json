{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Services Configuration",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "title": { "type": "string" },
      "price": { "type": "string" },
      "iconType": { "type": "string", "enum": ["Check", "Palette"] },
      "features": {
        "type": "array",
        "items": { "type": "string" }
      }
    },
    "required": ["title", "price", "iconType", "features"]
  }
}
